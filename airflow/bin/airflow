#!/usr/bin/env python
import logging
import os
from airflow.configuration import conf
from airflow.bin.cli import get_parser

if __name__ == '__main__':

    logging.root.handlers = []

    if conf.getboolean("security", "enabled"):
        os.environ['KRB5CCNAME'] = conf.get('security', 'ccache')
        os.environ['KRB5_KTNAME'] = conf.get('security', 'keytab')
        
    parser = get_parser()
    args = parser.parse_args()
    args.func(args)
